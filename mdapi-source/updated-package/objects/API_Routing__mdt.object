<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>This metadata defines what URLs can be called and what class will process the request posted to that URL</description>
    <label>API Routing</label>
    <pluralLabel>API Routings</pluralLabel>
    <visibility>Public</visibility>
    <fields>
        <fullName>API_URL__c</fullName>
        <description>Defines the API URL that can called by an external application.
The format is the defined base URL for the Apex REST API (for example /apigateway/v1/) + a specific destination like import.
Put together the value would be: /apigateway/v1/import</description>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <label>API URL</label>
        <length>255</length>
        <required>true</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Active__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Indicates if the route is active or not. When inactive the route will not be checked.</description>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Active</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Http_Method__c</fullName>
        <description>The Http method that can be used with the API URL</description>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <label>Http Method</label>
        <required>true</required>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>DELETE</fullName>
                    <default>true</default>
                    <label>DELETE</label>
                </value>
                <value>
                    <fullName>GET</fullName>
                    <default>false</default>
                    <label>GET</label>
                </value>
                <value>
                    <fullName>POST</fullName>
                    <default>false</default>
                    <label>POST</label>
                </value>
                <value>
                    <fullName>PUT</fullName>
                    <default>false</default>
                    <label>PUT</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>Service_Classname__c</fullName>
        <description>This field contains the classname of the class that will process the import message that is received at this url.
The class needs to import the Import_Service_Base class.</description>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <label>Service Classname</label>
        <length>255</length>
        <required>true</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
</CustomObject>
